<link href="${rc.contextPath}/_resources/css/bootstrap-select.css" rel="stylesheet">
<script src="${rc.contextPath}/_resources/js/bootstrap-select.js"></script>
<div class="ibox-content">
   <form class="form-horizontal" id="form1" action="doImport.html" method="POST" enctype="multipart/form-data">
         <div class="form-group">
              <label class="col-sm-3 control-label">广告列表：</label>
              <div class="col-sm-8" style="width:230px">
                <select name="advertId" class="selectpicker show-tick form-control">
                     #foreach($item in ${gameAdverts})
                        <option value="$!{item.advertId}">$!{item.advertName}</option>
                      #end
                  </select>
              </div>
          </div>
          <div class="form-group">
            <label class="col-sm-3 control-label">导入文件：</label>
            <div class="col-sm-8">
              <input type="file" name="file"  accept="application/vnd.ms-excel, application/vnd.openxmlformats-officedocument.spreadsheetml.sheet">
            </div>
         </div>
         <div class="form-group">
            <label class="col-sm-3 control-label">模板下载：</label>
            <div class="col-sm-8">
                <small class="text-navy"><a href="${rc.contextPath}/static/excel/channel_third_party.xlsx" class="btn btn-primary ">下载模板</a></small>
            </div>
        </div>
        <div align="center">
             <button class="btn btn-success" type="submit" ><i class="glyphicon glyphicon-ok"></i>导入</button>
             <button class="btn btn-white" type="button" onclick="parent.closeLayer()"><i class="glyphicon glyphicon-remove"></i>返回</button>
         </div>
     </form>
 </div>
 <script>
  #if($!{data})
     #if($!{data.code} == 1)
       parent.searchList();
       parent.closeLayer("$!{data.msg}");
     #else
       parent.layer.msg("$!{data.msg}",failIcon);
     #end
  #end
 </script>
</html>