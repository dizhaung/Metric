<script src="${rc.contextPath}/_resources/js/jquery.md5.js"></script>
<script src="${rc.contextPath}/_resources/js/plugins/layer/laydate/laydate.js"></script>
<div class="col-md-6">
    <div class="form-group">
        <div class="col-sm-9">
            <select data-placeholder="渠道名称..." class="chosen-select" style="width:350px;" title="">
                #foreach(${channel} in ${channelList})
                <option value="$!{channel.value}" hassubinfo="true">$!{channel.name}</option>
                #end
            </select>
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-9">
            <select data-placeholder="广告名称..." class="chosen-select" style="width:350px;" title="">
                #foreach(${ads} in ${adsList})
                <option value="$!{ads.value}" hassubinfo="true">$!{ads.name}</option>
                #end
            </select>
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-9">
            <select data-placeholder="广告主..." class="chosen-select" style="width:350px;" title="">
                #foreach(${adsOwner} in ${adsOwnerList})
                <option value="$!{adsOwner.value}" hassubinfo="true">$!{adsOwner.name}</option>
                #end
            </select>
        </div>
    </div>
</div>
<div class="col-md-6">
    <div class="form-group">
        <div class="col-sm-9">
            <select data-placeholder="结算类型..." class="chosen-select" style="width:350px;" title="">
                #foreach(${jiesuan} in ${jiesuanList})
                <option value="$!{jiesuan.value}" hassubinfo="true">$!{jiesuan.name}</option>
                #end
            </select>
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-9">
            <div class="input-daterange input-group date-picker input-daterange" id="datepicker">
                <input type="text" class="input-sm form-control" name="dateStart" id="dateStart" value="2017-1-1"/>
                <span class="input-group-addon">到</span>
                <input type="text" class="input-sm form-control" name="dateEnd" id="dateEnd" value="2017-1-1"/>
            </div>
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-9">
            <button type="button" class="btn btn-w-m btn-default" onclick="searchList('1')">搜索</button>
        </div>
    </div>
</div>
<div>
    <div class="jqGrid_wrapper">
        <table id="table_list_1"></table>
        <div id="pager_list_1"></div>
    </div>
</div>

<script>
  function searchList(page){
       $('#table_list_1').jqGrid('setGridParam',{
            url:'querySettlementData.html',
            page:page
       }).trigger("reloadGrid");
   }
   $(document).ready(function () {
        //,'渠道编号','广告名称','结算类型','结算指标','数值','单价','核减比例','结算金额','状态'
       $.jgrid.defaults.styleUI = 'Bootstrap';
       $('#table_list_1').jqGrid({
              url:'querySettlementData.html',
              datatype: "json",
              height: 500,
              autowidth: true,
              shrinkToFit: true,
              viewrecords:true,
              rowNum: 20,
              rowList: [20, 30, 50],
              colNames: ['id','序号','日期','渠道名称'],
              colModel: [
                 {
                   name: 'dataId',
                   hidden : true
                 },
                 {
                    name: 'dataName',
                    width: 10
                 },
                 {
                       name: 'dataName',
                       width: 10,
                  },
                  {
                      name: 'dataName',
                      width: 10
                  }
              ],
              pager: "#pager_list_1"
         });
   });




</script>