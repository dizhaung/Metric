<?xml version="1.0" encoding="UTF-8"?>
<!-- 添加 DUBBO SCHEMA -->
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:service timeout="${dubbo.service.timeout}" interface="org.loxf.metric.client.BoardService" ref="boardService"
                   version="${dubbo.service.version}"/>
    <dubbo:service timeout="${dubbo.service.timeout}" interface="org.loxf.metric.client.QuotaService" ref="quotaService"
                   version="${dubbo.service.version}"/>
    <dubbo:service timeout="${dubbo.service.timeout}" interface="org.loxf.metric.client.ChartService" ref="chartService"
                   version="${dubbo.service.version}"/>
    <dubbo:service timeout="${dubbo.service.timeout}" interface="org.loxf.metric.client.QuotaDimenValueService" ref="quotaDimenValueService"
                   version="${dubbo.service.version}"/>
    <dubbo:service timeout="${dubbo.service.timeout}" interface="org.loxf.metric.client.TargetService" ref="targetService"
                   version="${dubbo.service.version}"/>
</beans>